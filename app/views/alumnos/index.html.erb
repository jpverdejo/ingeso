<h1>Lista de alumnos</h1>

<table class="table table-striped table-condensed">
	<thead>
		<tr>
			<th>RUT</th>
			<th>Nombre</th>
			<th>Ingreso</th>
			<th>Grupo</th>
			<th>Seccion</th>
			<th>Foto</th>
			<th>Edit</th>
		</tr>
	</thead>
	<tbody>
		<% @alumnos.each do |alumno| %>
			<tr>
				<td><%= alumno.rut %></td>
				<td><%= alumno.nombre %></td>
				<td><%= "#{alumno.ingreso_ano}-#{alumno.ingreso_semestre}" %></td>
				<td><%= alumno.grupo %></td>
				<td><%= alumno.seccion %></td>
				<td><%= alumno.foto ? "Si" : "No" %></td>
				<td><a href="#">Edit</a></td>
			</tr>
		<% end %>
	</tbody>
</table>
